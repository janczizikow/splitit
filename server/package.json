{
  "name": "server",
  "scripts": {
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "build": "rimraf dist && tsc",
    "deploy": "prisma deploy"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "cookie-parser": "^1.4.3",
    "graphql-yoga": "1.16.9",
    "jsonwebtoken": "8.4.0",
    "nodemailer": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/cookie-parser": "^1.4.1",
    "@types/node": "10.12.18",
    "@types/nodemailer": "^4.6.5",
    "dotenv-cli": "1.4.0",
    "nodemon": "1.18.9",
    "prisma": "1.23.2",
    "rimraf": "2.6.2",
    "ts-node": "7.0.1",
    "typescript": "3.2.2"
  }
}
